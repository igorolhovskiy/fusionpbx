<context name="public">
	<extension name="click-to-call" continue="true" app_uuid="b1cd7509-5576-469a-892d-d0cfb66a4198" global="true">
		<condition field="click_to_call" expression="true">
			<action application="set" data="user_from_host=${domain_name}" inline="true"/>
			<action application="set" data="extension_uuid=${user_data ${sip_to_user}@${domain_name} var extension_uuid}" inline="true"/>
			<action application="set" data="call_timeout=${user_data ${sip_to_user}@${domain_name} var call_timeout}" inline="true"/>
			<action application="set" data="call_screen_enabled=${user_data ${sip_to_user}@${domain_name} var call_screen_enabled}" inline="true"/>
			<action application="set" data="accountcode=${user_data ${sip_to_user}@${domain_name} var accountcode}" inline="true"/>
			<action application="set" data="user_context=${user_data ${sip_to_user}@${domain_name} var user_context}" inline="true"/>
			<action application="set" data="outbound_caller_id_name=${user_data ${sip_to_user}@${domain_name} var outbound_caller_id_name}" inline="true"/>
			<action application="set" data="outbound_caller_id_number=${user_data ${sip_to_user}@${domain_name} var outbound_caller_id_number}" inline="true"/>
			<action application="set" data="directory-visible=${user_data ${sip_to_user}@${domain_name} var directory-visible}" inline="true"/>
			<action application="set" data="directory-exten-visible=${user_data ${sip_to_user}@${domain_name} var directory-exten-visible}" inline="true"/>
			<action application="set" data="limit_max=${user_data ${sip_to_user}@${domain_name} var limit_max}" inline="true"/>
			<action application="set" data="limit_destination=${user_data ${sip_to_user}@${domain_name} var limit_destination}" inline="true"/>
			<action application="set" data="extension_ringback=${user_data ${sip_to_user}@${domain_name} var extension_ringback}" inline="true"/>
			<action application="set" data="record_stereo=${user_data ${sip_to_user}@${domain_name} var record_stereo}" inline="true"/>
			<action application="set" data="transfer_fallback_extension=${user_data ${sip_to_user}@${domain_name} var transfer_fallback_extension}" inline="true"/>
			<action application="set" data="limit_destination=${user_data ${sip_to_user}@${domain_name} var limit_destination}" inline="true"/>
			<action application="set" data="number_alias=${user_data ${sip_to_user}@${domain_name} var number_alias}" inline="true"/>
			<anti-action application="set" data="user_from_host=${sip_from_host}" inline="true"/>
		</condition>
	</extension>
</context>
